{
  "name": "@obsidian-ai-providers/sdk",
  "version": "1.4.3",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig.json && node esbuild.config.mjs production",
    "dev": "node esbuild.config.mjs"
  },
  "description": "SDK for integrating with AI Providers plugin",
  "homepage": "https://github.com/pfrankov/obsidian-ai-providers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "obsidian",
    "ai",
    "sdk",
    "plugin",
    "llm",
    "gpt",
    "openai",
    "providers"
  ],
  "author": "Pavel Frankov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfrankov/obsidian-ai-providers.git"
  },
  "bugs": {
    "url": "https://github.com/pfrankov/obsidian-ai-providers/issues"
  },
  "peerDependencies": {
    "obsidian": "latest"
  },
  "devDependencies": {
    "esbuild": "0.17.3",
    "typescript": "4.7.4",
    "tslib": "2.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
